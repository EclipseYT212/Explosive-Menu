_G.AutoRobbing = nil 
if _G.AutoRobbing then return end
_G.AutoRobbing = true
if CheckPlane() then

	print("\nPlane Found")


	warn("Going to cargo plane")

	local crate = game:GetService("Workspace").Plane.Crates.Crate1:FindFirstChild("1").Position
	local teleport = loadstring(game:HttpGet("https://raw.githubusercontent.com/EclipseYT212/test3/main/tp"))();

	teleport(CFrame.new(crate));






	wait(2)
	warn("Going inside plane")
	local pos = Vector3.new(5-1169.8582763671875, 44.35041809082031, 2905.92236328125) -- your pos here
	local delay = 1 -- tp tween speed like how many seconds takes to tp

	-- do not edit below cuz breaks script

	local tween_s = game:GetService('TweenService')
	local tweeninfo = TweenInfo.new(delay,Enum.EasingStyle.Quad)

	local lp = game.Players.LocalPlayer

	if lp.Character and lp.Character:FindFirstChild('HumanoidRootPart') then
		local cf = CFrame.new(pos)
		local a = tween_s:Create(lp.Character.HumanoidRootPart,tweeninfo,{CFrame=cf})
		a:Play()



	end

	warn('Opening crates')
	wait(15)
	for i,v in pairs(require(game:GetService('ReplicatedStorage').Module.UI).CircleAction.Specs) do
		if v.Name == "Inspect Crate" then
			v:Callback(v, true)
		end
	end

	local VirtualInputManager = game:GetService('VirtualInputManager')

	VirtualInputManager:SendKeyEvent(true,"E",false,game)

	wait(30)


	Baseplate()
	warn("Bypassing by waiting")
	wait(15)
	warn("Selling")
	local teleport = loadstring(game:HttpGet("https://raw.githubusercontent.com/Introvert1337/RobloxReleases/main/Scripts/Jailbreak/Teleporation.lua"))();

	teleport(CFrame.new(-338.4594421386719, 21.26369285583496, 2055.239990234375));
	Baseplate()

	_G.AutoRobbing = false

else 






	workspace.ChildAdded:connect(function(v)

		if v.Name == "Drop" and v:IsA("Model") then
			if _G.AutoRobbing then return end
			_G.AutoRobbing = true
			warn("\nDrop found\nWaiting for it to land")


			wait(15)

			warn('Going to airdrop')
			local drop = game:GetService("Workspace").Drop.Top

			local teleport = loadstring(game:HttpGet("https://raw.githubusercontent.com/Introvert1337/RobloxReleases/main/Scripts/Jailbreak/Teleporation.lua"))();

			teleport(CFrame.new(drop.Position));






			wait(2)
			local target = game:GetService("Workspace").Drop.Top

			local Pathfind = game:GetService("PathfindingService")
			local balls = {}

			local Path = Pathfind:CreatePath()
			Path:ComputeAsync(game:GetService("Players").LocalPlayer.Character:WaitForChild("HumanoidRootPart").Position,target.Position)
			local Waypoint = Path:GetWaypoints()

			for _,v in pairs(Waypoint) do

				local ball = Instance.new("Part")
				ball.Shape = "Ball"
				ball.Material = "Neon"
				ball.Parent = game:GetService("Players").LocalPlayer.Character
				ball.Size = Vector3.new(0.6,0.6,0.6)
				ball.Anchored = true
				ball.Color = Color3.fromRGB(255,255,255)
				ball.Position = v.Position

				if v.Action == Enum.PathWaypointAction.Jump then
					game:GetService("Players").LocalPlayer.Character:WaitForChild("Humanoid"):ChangeState(Enum.HumanoidStateType.Jumping)
				end

				game:GetService("Players").LocalPlayer.Character:WaitForChild("Humanoid"):MoveTo(v.Position)
				table.insert(balls,ball)
			end

			for _,v in pairs(balls) do
				v.Color = Color3.fromRGB(255,0,0)
				wait(-9)
				v:Destroy() 
			end

		warn('Opening aridrop')


			local VirtualInputManager = game:GetService('VirtualInputManager')

			VirtualInputManager:SendKeyEvent(true,"E",false,game)
			game:GetService("Workspace").Drop.BriefcasePress:FireServer()
			wait(2)
			game:GetService("Workspace").Drop.NPCs:Destroy()





			-- Yield the coroutine
			--COllecting cash function
			wait(25)

			
			print("Collecting cash")



			local target = game:GetService("Workspace").DroppedCash.Cash.Part

			local Pathfind = game:GetService("PathfindingService")
			local balls = {}

			local Path = Pathfind:CreatePath()
			Path:ComputeAsync(game:GetService("Players").LocalPlayer.Character:WaitForChild("HumanoidRootPart").Position,target.Position)
			local Waypoint = Path:GetWaypoints()

			for _,v in pairs(Waypoint) do

				local ball = Instance.new("Part")
				ball.Shape = "Ball"
				ball.Material = "Neon"
				ball.Parent = game:GetService("Players").LocalPlayer.Character
				ball.Size = Vector3.new(0.6,0.6,0.6)
				ball.Anchored = true
				ball.Color = Color3.fromRGB(255,255,255)
				ball.Position = v.Position

				if v.Action == Enum.PathWaypointAction.Jump then
					game:GetService("Players").LocalPlayer.Character:WaitForChild("Humanoid"):ChangeState(Enum.HumanoidStateType.Jumping)
				end

				game:GetService("Players").LocalPlayer.Character:WaitForChild("Humanoid"):MoveTo(v.Position)
				table.insert(balls,ball)
			end

			for _,v in pairs(balls) do
				v.Color = Color3.fromRGB(255,0,0)
				wait()
				v:Destroy() 
			end
			local VirtualInputManager = game:GetService('VirtualInputManager')

			VirtualInputManager:SendKeyEvent(true,"E",false,game)

			wait(2)

			local target = game:GetService("Workspace").DroppedCash.Cash.Part

			local Pathfind = game:GetService("PathfindingService")
			local balls = {}

			local Path = Pathfind:CreatePath()
			Path:ComputeAsync(game:GetService("Players").LocalPlayer.Character:WaitForChild("HumanoidRootPart").Position,target.Position)
			local Waypoint = Path:GetWaypoints()

			for _,v in pairs(Waypoint) do

				local ball = Instance.new("Part")
				ball.Shape = "Ball"
				ball.Material = "Neon"
				ball.Parent = game:GetService("Players").LocalPlayer.Character
				ball.Size = Vector3.new(0.6,0.6,0.6)
				ball.Anchored = true
				ball.Color = Color3.fromRGB(255,255,255)
				ball.Position = v.Position

				if v.Action == Enum.PathWaypointAction.Jump then
					game:GetService("Players").LocalPlayer.Character:WaitForChild("Humanoid"):ChangeState(Enum.HumanoidStateType.Jumping)
				end

				game:GetService("Players").LocalPlayer.Character:WaitForChild("Humanoid"):MoveTo(v.Position)
				table.insert(balls,ball)
			end

			for _,v in pairs(balls) do
				v.Color = Color3.fromRGB(255,0,0)
				wait()
				v:Destroy() 
			end
			local VirtualInputManager = game:GetService('VirtualInputManager')

			VirtualInputManager:SendKeyEvent(true,"E",false,game)



			wait(2)

			local target = game:GetService("Workspace").DroppedCash.Cash.Part

			local Pathfind = game:GetService("PathfindingService")
			local balls = {}

			local Path = Pathfind:CreatePath()
			Path:ComputeAsync(game:GetService("Players").LocalPlayer.Character:WaitForChild("HumanoidRootPart").Position,target.Position)
			local Waypoint = Path:GetWaypoints()

			for _,v in pairs(Waypoint) do

				local ball = Instance.new("Part")
				ball.Shape = "Ball"
				ball.Material = "Neon"
				ball.Parent = game:GetService("Players").LocalPlayer.Character
				ball.Size = Vector3.new(0.6,0.6,0.6)
				ball.Anchored = true
				ball.Color = Color3.fromRGB(255,255,255)
				ball.Position = v.Position

				if v.Action == Enum.PathWaypointAction.Jump then
					game:GetService("Players").LocalPlayer.Character:WaitForChild("Humanoid"):ChangeState(Enum.HumanoidStateType.Jumping)
				end

				game:GetService("Players").LocalPlayer.Character:WaitForChild("Humanoid"):MoveTo(v.Position)
				table.insert(balls,ball)
			end

			for _,v in pairs(balls) do
				v.Color = Color3.fromRGB(255,0,0)
				wait()
				v:Destroy() 
			end
			wait(5)
			print('Airdrop Completed')
			Baseplate()
			_G.AutoRob = false
		end
	end)
	
	end



function antiafk()
	local vu = game:GetService("VirtualUser")
	game:GetService("Players").LocalPlayer.Idled:connect(function()
		vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
		wait(1)
		vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
	end)

end

function Baseplate()
	local baseplate = Instance.new("Part")
	baseplate.Parent = workspace
	baseplate.Size = Vector3.new(100,5,100) -- change size
	baseplate.Anchored = true
	baseplate.Position = game.Players.LocalPlayer.Character.HumanoidRootPart.Position + Vector3.new(0,650,0) -- higher number for it to spawn higher
	wait(1)
	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = baseplate.CFrame + Vector3.new(0,10,0)

end

function fireworks()
	for i, v in pairs(getgc()) do
		if typeof(v) == "function" then
			for i2, v2 in pairs(debug.getupvalues(v)) do
				if typeof(v2) == "table" and rawget(v2, "Fireworks") then
					v2.Fireworks(math.huge)
				end
			end
		end
	end

end

function CheckTrainType()
	for i,v in pairs(workspace.Trains:GetChildren()) do
		if v.Name == "BoxCar" then
			return v.Model.Rob
		elseif v.Name == "SteamEngine" then
			return "Passenger"
		end
	end
end

function CheckPlane()
	for i,v in pairs(workspace:GetChildren()) do
		if v.Name == "Plane" and v.Root then
			if v.Crates.Crate1["1"].Transparency == 0 then
				return v.Crates:GetChildren()[math.random(1,table.maxn(v.Crates:GetChildren()))]
			else
				return false
			end
		end
	end
end
